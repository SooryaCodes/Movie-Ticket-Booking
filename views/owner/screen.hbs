<script>
  function deleteScreen(Id) {
    var confirmDelete = confirm('Are You Sure Want To Delete')
    if (confirmDelete) {
      $.ajax({
        url: '/owner/delete-screen/' + Id,
        data: Id,
        method: 'post',
        success: (response) => {

          console.log(response.status)
          if (response.status) {
            swal({
              title: "Deleted",
              text: "Successfully",
              icon: "success",
              button: false
            }); setTimeout(function () {
              location.reload()
            }, 2000)


          }
        }
      })
    }

  }
</script>  <script>
  var loader = document.querySelector('.loader')
  setTimeout(function () {

    loader.classList.remove('active')
  }, 2000)  </script>